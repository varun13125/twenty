{
  "buildCommand": "cd packages/twenty-front && yarn build",
  "outputDirectory": "packages/twenty-front/build",
  "installCommand": "rm -f yarn.lock && yarn install",
  "framework": "vite",
  "env": {
    "NODE_ENV": "production",
    "VITE_DISABLE_TYPESCRIPT_CHECKER": "true",
    "VITE_DISABLE_ESLINT_CHECKER": "true"
  }
} 